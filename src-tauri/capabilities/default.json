{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Kotoori デフォルト権限セット",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:scope-app-recursive",
    "fs:scope-applocaldata-recursive",
    "fs:scope-home-recursive",
    "fs:scope-desktop-recursive",
    "fs:scope-document-recursive",
    "fs:scope-download-recursive",
    "shell:allow-open",
    "clipboard-manager:default",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-write-text",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-load",
    "store:allow-save",
    "store:allow-keys",
    "store:allow-entries",
    "store:allow-has",
    "store:allow-length",
    "window-state:default",
    "window-state:allow-restore-state",
    "window-state:allow-save-window-state",
    "os:default",
    "os:allow-locale",
    "os:allow-platform"
  ]
}
